<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/orange" >
    
    <!--       个人设置                             -->
    
    <RelativeLayout 
        android:id="@+id/my_center_self_info_layout"
        android:layout_width="fill_parent"
   	 	android:layout_height="wrap_content"
   	 	android:layout_margin="10dp"
   	 	android:background="@drawable/shape_my_center_green_round_bg"
        >
        <ImageView 
            android:id="@+id/my_center_head_icon_imageview"
            android:layout_width="46dp"
   	 		android:layout_height="46dp"
   	 		android:layout_marginTop="20dp"
   	 		android:layout_marginBottom="20dp"
   	 		android:layout_marginLeft="12dp"
   	 		android:scaleType="fitXY"
   	 		android:src="@drawable/default_head_icon"
   	 		android:layout_alignParentLeft="true"
   	 		android:layout_centerVertical="true"
            />
         <ImageView 
            android:id="@+id/my_center_arrow_icon_1"
            android:layout_width="20dp"
   	 		android:layout_height="20dp"
   	 		android:layout_marginRight="14dp"
   	 		android:layout_alignParentRight="true"
   	 		android:layout_centerVertical="true"
   	 		android:src="@drawable/my_center_arrow_icon"/>
       
          <TextView 
            android:id="@+id/my_center_user_name_textview"
            android:layout_width="wrap_content"
   	 		android:layout_height="22dp"
   	 		android:layout_toRightOf="@id/my_center_head_icon_imageview"
   	 		android:layout_toLeftOf="@id/my_center_arrow_icon_1"
   	 		android:gravity="center_vertical"
   	 		android:layout_marginLeft="16dp"
   	 		android:layout_marginTop="10dp"
   	 		android:textColor="@color/green_textcolor"
   	 		android:textSize="18sp"/>
        
          <TextView 
            android:id="@+id/my_center_user_account_textview"
            android:layout_width="wrap_content"
   	 		android:layout_height="20dp"
   	 		android:layout_toRightOf="@id/my_center_head_icon_imageview"
   	 		android:layout_toLeftOf="@id/my_center_arrow_icon_1"
   	 		android:layout_below="@id/my_center_user_name_textview"
   	 		android:gravity="center_vertical"
   	 		android:layout_marginLeft="16dp"
   	 		android:layout_marginTop="4dp"
   	 		android:textColor="@color/gray_textcolor"
   	 		android:textSize="14sp"/>
    </RelativeLayout>
    
    
        <!--      我的宝宝\我关注的宝宝                 -->
     <RelativeLayout 
        android:layout_width="fill_parent"
   	 	android:layout_height="104dp"
   	 	android:layout_margin="10dp"
   	 	android:background="@drawable/shape_my_center_green_round_bg">
    	
         <RelativeLayout 
             android:id="@+id/my_center_my_baby_layout"
             android:layout_width="fill_parent"
   	 		 android:layout_height="30dp"
   	 		 android:gravity="center_vertical"
   	 		 android:layout_marginTop="10dp"
   	 		 android:layout_marginBottom="10dp">
              <ImageView 
		            android:id="@+id/my_center_my_baby_imageview"
		            android:layout_width="30dp"
		   	 		android:layout_height="30dp"
		   	 		android:layout_marginLeft="8dp"
		   	 		android:layout_alignParentLeft="true"
		   	 		android:layout_centerVertical="true"
		   	 		android:src="@drawable/my_center_my_baby"/>
             <ImageView 
		            android:id="@+id/my_center_arrow_icon_2"
		            android:layout_width="20dp"
		   	 		android:layout_height="20dp"
		   	 		android:layout_marginRight="12dp"
		   	 		android:layout_alignParentRight="true"
		   	 		android:layout_centerVertical="true"
		   	 		android:src="@drawable/my_center_arrow_icon"/>
             <TextView 
		            android:id="@+id/my_center_my_baby_textview"
		            android:layout_width="wrap_content"
		   	 		android:layout_height="wrap_content"
		   	 		android:layout_marginBottom="6dp"
		   	 		android:layout_toRightOf="@id/my_center_my_baby_imageview"
		   	 		android:layout_toLeftOf="@id/my_center_arrow_icon_2"
		   	 		android:gravity="center_vertical"
		   	 		android:layout_centerVertical="true"
		   	 		android:layout_marginLeft="14dp"
		   	 		android:text="我的宝宝"
		   	 		android:textColor="@color/green_textcolor"
		   	 		android:textSize="18sp"/>
         </RelativeLayout>
         
         
         <LinearLayout 
             android:id="@+id/my_center_my_baby_divider"
             android:layout_width="fill_parent"
   	 		 android:layout_height="1dp"
   	 		 android:layout_below="@id/my_center_my_baby_layout"
   	 		 android:background="@color/green_divider"
   	 		 android:orientation="horizontal"></LinearLayout>
         
         
         <RelativeLayout 
             android:id="@+id/my_center_my_care_baby_layout"
             android:layout_width="fill_parent"
   	 		 android:layout_height="wrap_content"
   	 		 android:layout_below="@id/my_center_my_baby_divider"
   	 		 android:gravity="center_vertical"
   	 		 android:layout_marginTop="12dp"
   	 		 android:layout_marginBottom="10dp">
              <ImageView 
		            android:id="@+id/my_center_my_care_baby_imageview"
		            android:layout_width="30dp"
		   	 		android:layout_height="30dp"
		   	 		android:layout_marginLeft="8dp"
		   	 		android:layout_alignParentLeft="true"
		   	 		android:layout_centerVertical="true"
		   	 		android:src="@drawable/my_center_my_care_baby"/>
             <ImageView 
		            android:id="@+id/my_center_arrow_icon_3"
		            android:layout_width="20dp"
		   	 		android:layout_height="20dp"
		   	 		android:layout_marginRight="12dp"
		   	 		android:layout_alignParentRight="true"
		   	 		android:layout_centerVertical="true"
		   	 		android:src="@drawable/my_center_arrow_icon"/>
             <TextView 
		            android:id="@+id/my_center_my_care_baby_textview"
		            android:layout_width="wrap_content"
		   	 		android:layout_height="wrap_content"
		   	 		android:layout_marginTop="4dp"
		   	 		android:layout_toRightOf="@id/my_center_my_care_baby_imageview"
		   	 		android:layout_toLeftOf="@id/my_center_arrow_icon_3"
		   	 		android:layout_marginLeft="14dp"
		   	 		android:text="我关注的宝宝"
		   	 		android:textColor="@color/green_textcolor"
		   	 		android:textSize="18sp"/>
         </RelativeLayout>
         
    </RelativeLayout>
    
     <!--          我的 积分\收藏                          -->
  	<RelativeLayout 
        android:layout_width="fill_parent"
  	 	android:layout_height="104dp"
  	 	android:layout_margin="10dp"
  	 	android:background="@drawable/shape_my_center_green_round_bg">
    	
         <RelativeLayout 
             android:id="@+id/my_center_my_points_layout"
             android:layout_width="fill_parent"
   	 		 android:layout_height="30dp"
   	 		 android:gravity="center_vertical"
   	 		 android:layout_marginTop="10dp"
   	 		 android:layout_marginBottom="10dp">
              <ImageView 
		            android:id="@+id/my_center_my_points_imageview"
		            android:layout_width="30dp"
		   	 		android:layout_height="30dp"
		   	 		android:layout_marginLeft="8dp"
		   	 		android:layout_alignParentLeft="true"
		   	 		android:layout_centerVertical="true"
		   	 		android:src="@drawable/my_center_points"/>
             <ImageView 
		            android:id="@+id/my_center_arrow_icon_4"
		            android:layout_width="20dp"
		   	 		android:layout_height="20dp"
		   	 		android:layout_marginRight="12dp"
		   	 		android:layout_alignParentRight="true"
		   	 		android:layout_centerVertical="true"
		   	 		android:src="@drawable/my_center_arrow_icon"/>
             <TextView 
		            android:id="@+id/my_center_my_points_textview"
		            android:layout_width="wrap_content"
		   	 		android:layout_height="wrap_content"
		   	 		android:layout_marginBottom="6dp"
		   	 		android:layout_toRightOf="@id/my_center_my_points_imageview"
		   	 		android:layout_toLeftOf="@id/my_center_arrow_icon_4"
		   	 		android:gravity="center_vertical"
		   	 		android:layout_centerVertical="true"
		   	 		android:layout_marginLeft="14dp"
		   	 		android:text="我的积分"
		   	 		android:textColor="@color/green_textcolor"
		   	 		android:textSize="18sp"/>
         </RelativeLayout>
         
         
         <LinearLayout 
             android:id="@+id/my_center_my_points_divider"
             android:layout_width="fill_parent"
   	 		 android:layout_height="1dp"
   	 		 android:layout_below="@id/my_center_my_points_layout"
   	 		 android:background="@color/green_divider"
   	 		 android:orientation="horizontal"></LinearLayout>
         
         <RelativeLayout 
             android:id="@+id/my_center_my_favorite_layout"
             android:layout_width="fill_parent"
   	 		 android:layout_height="wrap_content"
   	 		 android:layout_below="@id/my_center_my_points_divider"
   	 		 android:gravity="center_vertical"
   	 		 android:layout_marginTop="12dp"
   	 		 android:layout_marginBottom="10dp">
              <ImageView 
		            android:id="@+id/my_center_my_favorite_imageview"
		            android:layout_width="30dp"
		   	 		android:layout_height="30dp"
		   	 		android:layout_marginLeft="8dp"
		   	 		android:layout_alignParentLeft="true"
		   	 		android:layout_centerVertical="true"
		   	 		android:src="@drawable/my_center_favorite"/>
             <ImageView 
		            android:id="@+id/my_center_arrow_icon_5"
		            android:layout_width="20dp"
		   	 		android:layout_height="20dp"
		   	 		android:layout_marginRight="12dp"
		   	 		android:layout_alignParentRight="true"
		   	 		android:layout_centerVertical="true"
		   	 		android:src="@drawable/my_center_arrow_icon"/>
             <TextView 
		            android:id="@+id/my_center_my_favorite_textview"
		            android:layout_width="wrap_content"
		   	 		android:layout_height="wrap_content"
		   	 		android:layout_marginTop="4dp"
		   	 		android:layout_toRightOf="@id/my_center_my_favorite_imageview"
		   	 		android:layout_toLeftOf="@id/my_center_arrow_icon_5"
		   	 		android:layout_marginLeft="14dp"
		   	 		android:text="我的收藏"
		   	 		android:textColor="@color/green_textcolor"
		   	 		android:textSize="18sp"/>
         </RelativeLayout>
         
    </RelativeLayout>
    
	<!--          设置                                          -->
     <RelativeLayout 
         android:id="@+id/my_center_setting_layout"
         android:layout_width="fill_parent"
   	 	 android:layout_height="52dp"
   	 	 android:layout_margin="10dp"
   	 	 android:gravity="center_vertical"
   	 	 android:background="@drawable/shape_my_center_green_round_bg">
    	<ImageView 
            android:id="@+id/my_center_settting_imageview"
            android:layout_width="36dp"
   	 		android:layout_height="36dp"
   	 		android:layout_marginLeft="8dp"
   	 		android:layout_alignParentLeft="true"
   	 		android:layout_centerVertical="true"
   	 		android:src="@drawable/my_center_settting"/>
        <ImageView 
            android:id="@+id/my_center_arrow_icon_6"
            android:layout_width="20dp"
   	 		android:layout_height="20dp"
   	 		android:layout_marginRight="12dp"
   	 		android:layout_alignParentRight="true"
   	 		android:layout_centerVertical="true"
   	 		android:src="@drawable/my_center_arrow_icon"/>
        <TextView 
            android:id="@+id/my_center_settting_textview"
            android:layout_width="wrap_content"
   	 		android:layout_height="wrap_content"
   	 		android:layout_centerVertical="true"
   	 		android:layout_marginTop="8dp"
   	 		android:layout_toRightOf="@id/my_center_settting_imageview"
   	 		android:layout_toLeftOf="@id/my_center_arrow_icon_6"
   	 		android:layout_marginLeft="14dp"
   	 		android:text="设置"
   	 		android:textColor="@color/green_textcolor"
   	 		android:textSize="18sp"/>
    </RelativeLayout>
</LinearLayout>
